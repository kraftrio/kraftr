{
  "name": "@kraftr/sequence",
  "version": "0.1.2",
  "type": "module",
  "homepage": "https://github.com/kraftrio/kraftr",
  "repository": {
    "directory": "core/sequence",
    "type": "git",
    "url": "https://github.com/kraftrio/kraftr"
  },
  "bugs": {
    "url": "https://github.com/kraftrio/kraftr/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "craft build",
    "build:watch": "craft build --silent --watch",
    "stub": "craft build --silent --stub",
    "test": "npm run test:files && npm run test:types",
    "test:watch": "vitest dev --reporter=dot",
    "test:files": "vitest run --reporter=dot",
    "test:types": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "nodejs",
    "framework"
  ],
  "sideEffects": false,
  "dependencies": {
    "@kraftr/common": "workspace:^0.2.1",
    "@kraftr/context": "workspace:^0.2.1",
    "toposort": "^2.0.2"
  },
  "devDependencies": {
    "@kraftr/build": "workspace:^0.6.0",
    "@kraftr/eslint-plugin": "workspace:^0.4.0",
    "@types/node": "^16.11.7",
    "@types/toposort": "^2.0.3",
    "eslint": "^8.2.0",
    "tslib": "^2.3.1",
    "vite": "^3.0.0",
    "vitest": "latest"
  }
}
