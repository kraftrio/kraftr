{
  "name": "@kraftr/eslint-plugin",
  "version": "0.1.0",
  "description": "",
  "files": [
    "dist/"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Michel Betancourt",
  "scripts": {
    "build": "tsup --format cjs,esm ./src/index.ts ./src/eslint.ts --dts-resolve",
    "build:watch": "tsup --format cjs,esm ./src/index.ts ./src/eslint.ts --dts-resolve",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:types": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "typescript": "4.4.4",
    "@typescript-eslint/experimental-utils": "5.3.0",
    "tsutils": "3.21.0",
    "deepmerge": "4.2.2",
    "json-schema": "0.3.0",
    "@typescript-eslint/scope-manager": "5.3.0",
    "@typescript-eslint/types": "5.1.0",
    "@typescript-eslint/typescript-estree": "5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "@rushstack/eslint-patch": "~1.0.8",
    "eslint-plugin-write-good-comments": "~0.1.3"
  },
  "peerDependencies": {
    "eslint": "^7.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/estree": "0.0.50",
    "tsup": "5.5.0",
    "@types/eslint": "7.28.2",
    "@jest/types": "27.2.5",
    "jest": "27.3.1",
    "esbuild": "0.13.10",
    "esbuild-jest": "0.5.0",
    "eslint-plugin-security-node": "1.0.14",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-problems": "5.0.0",
    "eslint-config-hardcore": "23.0.0",
    "eslint-plugin-sonarjs": "0.10.0",
    "eslint-plugin-unicorn": "37.0.1",
    "eslint": "^7.0.0",
    "tslib": "2.3.1",
    "eslint-plugin-unused-imports": "1.1.5"
  }
}
